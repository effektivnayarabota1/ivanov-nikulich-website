<form
	action="/admin/create-page"
	method="POST"
	enctype="multipart/form-data"
	id="create"
>
	<fieldset>
		<legend>Мета</legend>
		<input type="text" name="pageId" placeholder="id" required />
		<input type="text" name="title" placeholder="title" required />
		<textarea
			name="desc"
			form="create"
			placeholder="desc"
			required
		></textarea>
		<input type="file" name="cover" required />
	</fieldset>
	<input type="submit" value="Сохранить" />
</form>
